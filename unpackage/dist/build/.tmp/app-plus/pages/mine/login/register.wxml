<view class="register"><view class="content"><view class="header"><image src="{{logoImage}}"></image><view class="but-fail" type="primary" data-event-opts="{{[['tap',[['handleFail',['$event']]]]]}}" bindtap="__e"><image src="../../../static/icons/modify.png" mode></image>上传头像</view></view><view class="main"><w-input bind:input="__e" vue-id="1" type="text" maxlength="11" placeholder="用户名" value="{{username}}" data-event-opts="{{[['^input',[['__set_model',['','username','$event',[]]]]]]}}" bind:__l="__l"></w-input><w-input bind:input="__e" vue-id="2" type="password" maxlength="11" placeholder="登录密码" isShowPass value="{{passData}}" data-event-opts="{{[['^input',[['__set_model',['','passData','$event',[]]]]]]}}" bind:__l="__l"></w-input><w-input bind:input="__e" vue-id="3" type="text" maxlength="11" placeholder="手机号" value="{{phoneData}}" data-event-opts="{{[['^input',[['__set_model',['','phoneData','$event',[]]]]]]}}" bind:__l="__l"></w-input><w-input bind:input="__e" vue-id="4" type="text" maxlength="11" placeholder="邮箱" value="{{mailbox}}" data-event-opts="{{[['^input',[['__set_model',['','mailbox','$event',[]]]]]]}}" bind:__l="__l"></w-input><w-input class="vue-ref" vue-id="5" type="number" maxlength="4" placeholder="验证码" isShowCode data-ref="runCode" value="{{verCode}}" data-event-opts="{{[['^setCode',[['getVerCode']]],['^input',[['__set_model',['','verCode','$event',[]]]]]]}}" bind:setCode="__e" bind:input="__e" bind:__l="__l"></w-input></view><w-button vue-id="6" text="注 册" rotate="{{isRotate}}" data-event-opts="{{[['tap',[['startReg']]]]}}" bindtap="__e" bind:__l="__l"></w-button><view class="footer"><text data-event-opts="{{[['tap',[['isShowAgree',['$event']]]]]}}" class="{{['cuIcon',showAgree?'cuIcon-radiobox':'cuIcon-round']}}" bindtap="__e">同意</text><navigator url open-type="navigate">《协议》</navigator></view></view></view>